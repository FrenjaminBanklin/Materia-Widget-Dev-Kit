{
  "name": "materia-widget-development-kit",
  "description": "Materia Widget Development Kit (MDK) is a local dev environment for quickly building Materia Widgets.",
  "license": "AGPL-3.0",
  "homepage": "https://ucfopen.github.io/Materia-Docs/",
  "version": "1.2.2",
  "author": "University of Central Florida, Center for Distributed Learning",
  "scripts": {
    "build": "webpack",
    "start": "env TEST_MDK=true webpack-dev-server --open --config webpack.config.widget.js",
    "postinstall": "npm run build",
    "clientAssets:link": "cd ../materia-server-client-assets && yarn link && cd - && yarn link materia-server-client-assets"
  },
  "materia": {
    "cleanName": "MDK"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "coffee-loader": "^0.7.3",
    "coffee-script": "^1.12.7",
    "copy-webpack-plugin": "^4.0.1",
    "express": "^4.16.0",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "hogan-express": "^0.5.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.29.0",
    "materia-client-assets": "https://github.com/ucfopen/materia-server-client-assets.git",
    "node-sass": "^4.5.3",
    "postcss-loader": "2.0.6",
    "raw-loader": "^0.5.1",
    "string-replace-loader": "^1.3.0",
    "wait-until-promise": "^1.0.0",
    "webpack-dev-server": "^2.6.1",
    "yamljs": "^0.2.8",
    "zip-webpack-plugin": "^2.0.0"
  }
}
